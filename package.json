{
  "name": "tiny-date-format",
  "version": "0.1.1",
  "description": "A small library (around 400 B when gziped & minified) to format JavaScript `Date` object using same tokens as moment.",
  "cdn": "dist/tiny-date-format.umd.min.js",
  "main": "dist/tiny-date-format.js",
  "unpkg": "dist/tiny-date-format.umd.min.js",
  "types": "types/tiny-date-format.d.ts",
  "module": "dist/tiny-date-format.esm.js",
  "browser": "dist/tiny-date-format.umd.min.js",
  "jsdelivr": "dist/tiny-date-format.umd.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "types/",
    "dist/"
  ],
  "scripts": {
    "lint": "prettier --check \"./src/**/*.ts\"",
    "lint:fix": "prettier --write \"./src/**/*.ts\"",
    "build": "tsc && bili src/tiny-date-format.js --format es,cjs,umd-min --module-name tinyDateFormat --minimal",
    "test": "pnpm run lint && ava",
    "pretest": "pnpm run build",
    "prepublishOnly": "pnpm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/tiny-date-format.git"
  },
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/tiny-date-format/issues"
  },
  "homepage": "https://github.com/VitorLuizC/tiny-date-format",
  "devDependencies": {
    "ava": "^2.4.0",
    "bili": "^4.8.1",
    "prettier": "^2.0.5",
    "typescript": "^3.6.4"
  }
}
